		"drizzle-kit": "^0.31.7",
		"drizzle-orm": "^0.44.7",
		"svelte": "^5.43.8",
		"svelte-check": "^4.3.4",
		"typescript": "^5.9.3",
		"vite": "^7.2.2",
		"tsx": "^4.21.0"
